<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>

	<package name="userAccess" namespace="/access" extends="struts-default">

		<action name="user_*" class="userStatusAction" method="{1}">
			<result name="success">
				/user/User_{1}_success.jsp
			</result>
			<result name="error">
				/user/User_{1}_fail.jsp
			</result>
		</action>
	</package>

	<package name="userOperation" namespace="/operation">
		<action name="user_*" class="userSignInAction" method="{1}">
			
		</action>
	</package>

</struts>