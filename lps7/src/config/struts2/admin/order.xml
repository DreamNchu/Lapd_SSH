<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	
	<package name="order" namespace="/admin" extends="json-default">
		
		<action name="createOrder" class="createOrderAction">
			<result type="json">
				<param name="root">result</param>
			</result>
		</action>
		
		<action name="initOrderData" class="initCreateOrderBasicDataAction" method="initOrderData">
			<result type="json">
				<param name="root">result</param>
			</result>
		</action>
		
		<action name="initOrder" class="initCreateOrderBasicDataAction" >
			<result>
				/admin/initOrder.jsp
			</result>
		</action>
		
		
	</package>


</struts>