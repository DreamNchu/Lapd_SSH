<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>

	<package name="adminAccess" namespace="/access" extends="struts-default">
		<action name="admin_*" class="adminStatusAction" method="{1}">
			<result name="success">
				/admin/admin_{1}_success.jsp
			</result>
			<result name="error">
				/admin/admin_{1}_fail.jsp
			</result>
		</action>
	</package>

</struts>