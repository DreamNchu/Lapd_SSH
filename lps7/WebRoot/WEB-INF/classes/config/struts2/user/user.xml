<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	<package name="user" namespace="/user" extends="json-default,userLogin">
		<action name="modifyUserDataPage" class="userDataAction">
			<result>
				/user/modifyUserDataPage.jsp
			</result>
		</action>
		
			<action name="userData" class="userDataAction" method="userData">
			<result>
				/user/userData.jsp
			</result>
		</action>
		
		<action name="modifyData" class="userDataAction" method="modifyData">
			<result type="json">
				<param name="root">result</param>
			</result>
		</action>
	</package>

</struts>